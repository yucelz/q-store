# Q-Store Examples Environment Configuration
# Copy this file to .env and fill in your credentials

# IonQ Configuration (for real quantum hardware/simulator)
IONQ_API_KEY=your_ionq_api_key_here
IONQ_TARGET=simulator  # Options: simulator, qpu.harmony, qpu.aria-1

# Pinecone Configuration (for quantum database features)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=us-east-1  # or your Pinecone environment

# Backend Configuration
# Use 'mock_ideal' for testing without real hardware
# Use 'ionq_simulator' or 'ionq_hardware' for real connections
DEFAULT_BACKEND=mock_ideal
